[config]
; 全局配置文件 - 程序启动时会首先加载此文件
; 此文件用于指定各个模块的配置文件路径

[Paths]
; AI部分配置文件
AIPart = c:/Users/Administrator/Documents/GitHub/CompearProject/ConfigDir/AIPart.dir

; 硬件监控配置文件
Hardware = c:/Users/Administrator/Documents/GitHub/CompearProject/ConfigDir/Hardwaer.dir

; 主程序代码配置文件
MainCode = c:/Users/Administrator/Documents/GitHub/CompearProject/ConfigDir/MainCode.dir

; Web服务配置文件
Web = c:/Users/Administrator/Documents/GitHub/CompearProject/ConfigDir/Web.dir

[Global]
; 配置文件格式 (当前为JSON)
Format = JSON

; 是否启用配置热重载
HotReload = false

; 配置热重载间隔 (秒)
ReloadInterval = 300

; 默认日志级别
DefaultLogLevel = INFO